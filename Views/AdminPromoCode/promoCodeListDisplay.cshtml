@model List<TheBookCave.Models.ViewModels.PromoCodeListViewModel> 

@{
    Layout = "_AdminLayout";
}

<h1>All promoCode </h1>


<h2> PromoCodeList </h2>

<div>
    @foreach(var promoCode in @Model)
    {  
        <ul>
        <li>promoCodeId: @promoCode.Id</li>
        <li>Discount: @promoCode.Discount</li>
        <li>Description: @promoCode.Description</li>
        <li>Published: @promoCode.Published</li>
        </ul>
    }
</div>