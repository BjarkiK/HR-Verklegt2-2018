@model List<TheBookCave.Models.ViewModels.SubscriptionListViewModel> 

@{
    Layout = "_AdminLayout";
}

<h1>All subscription </h1>


<h2> SubscriptionList </h2>

<div>
    @foreach(var subscription in @Model)
    {  
        <ul>
        <li>subscriptionId: @subscription.Id</li>
        <li>DescriptionEn: @subscription.DescriptionEn</li>
        <li>DescriptionIn: @subscription.DescriptionIn</li>
        <li>TypeEn: @subscription.TypeEn</li>
        <li>TypeIn: @subscription.TypeIn</li>
        </ul>
    }
</div>
